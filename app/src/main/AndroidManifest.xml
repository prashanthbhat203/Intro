                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Form" />
        <activity android:name=".Signup_Form" />
        <activity android:name=".Succesfull_Login" />
    </application>

</manifest>